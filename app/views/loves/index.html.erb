<p id="notice"><%= notice %></p>

<h1>Loves</h1>

<table>
  <thead>
    <tr>
      <th>Image1</th>
      <th>Image2</th>
      <th>Image3</th>
      <th>Image4</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @loves.each do |lofe| %>
      <tr>
        <td><%= lofe.image1 %></td>
        <td><%= lofe.image2 %></td>
        <td><%= lofe.image3 %></td>
        <td><%= lofe.image4 %></td>
        <td><%= link_to 'Show', lofe %></td>
        <td><%= link_to 'Edit', edit_lofe_path(lofe) %></td>
        <td><%= link_to 'Destroy', lofe, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lofe', new_lofe_path %>
