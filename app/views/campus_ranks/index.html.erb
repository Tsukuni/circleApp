<p id="notice"><%= notice %></p>

<h1>Campus Ranks</h1>

<table>
  <thead>
    <tr>
      <th>Rank1</th>
      <th>Rank2</th>
      <th>Rank3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campus_ranks.each do |campus_rank| %>
      <tr>
        <td><%= campus_rank.rank1 %></td>
        <td><%= campus_rank.rank2 %></td>
        <td><%= campus_rank.rank3 %></td>
        <td><%= link_to 'Show', campus_rank %></td>
        <td><%= link_to 'Edit', edit_campus_rank_path(campus_rank) %></td>
        <td><%= link_to 'Destroy', campus_rank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Campus Rank', new_campus_rank_path %>
