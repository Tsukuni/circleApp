<div class="col-md-12">
  <h3>Men</h3>
</div>
<div>
  <%=form_for @men_snap do |f| %>
  <div class="col-md-6 adBox">
    <table class="table">
      <tr>
        <th scope="row">画像</th>
        <td><%= f.file_field :men_image %></td>
      </tr>
      <tr>
        <th scope="row">名前</th>
        <td><%= f.text_field :name %></td>
      </tr>
    </table>
    <%= f.submit "送信" ,class: "btn button" %>
  </div>
  <% end %>
</div>
