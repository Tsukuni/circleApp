<p id="notice"><%= notice %></p>

<h1>School Ranks</h1>

<table>
  <thead>
    <tr>
      <th>Qestion1</th>
      <th>Qestion2</th>
      <th>Qt1 rank1</th>
      <th>Qt1 rank2</th>
      <th>Qt1 rank3</th>
      <th>Qt2 rank1</th>
      <th>Qt2 rank2</th>
      <th>Qt2 rank3</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @school_ranks.each do |school_rank| %>
      <tr>
        <td><%= school_rank.qestion1 %></td>
        <td><%= school_rank.qestion2 %></td>
        <td><%= school_rank.qt1_rank1 %></td>
        <td><%= school_rank.qt1_rank2 %></td>
        <td><%= school_rank.qt1_rank3 %></td>
        <td><%= school_rank.qt2_rank1 %></td>
        <td><%= school_rank.qt2_rank2 %></td>
        <td><%= school_rank.qt2_rank3 %></td>
        <td><%= school_rank.content %></td>
        <td><%= link_to 'Show', school_rank %></td>
        <td><%= link_to 'Edit', edit_school_rank_path(school_rank) %></td>
        <td><%= link_to 'Destroy', school_rank, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New School Rank', new_school_rank_path %>
