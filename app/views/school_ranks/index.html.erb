<h3>ランキング</h3>
  <div class="col-md-5 adBox">
    <h4>学校</h4>
    <% @school_ranks.each do |school_rank| %>
      <table class="table">
        <tr>
          <th scope="row">質問1</th>
          <td><%= school_rank.qestion1 %>
          <span class="button"><%= link_to '編集',edit_school_rank_path(school_rank)%></td>
        </tr>
        <tr>
          <th scope="row">1位</th>
          <td><%= school_rank.qt1_rank1 %></td>
        </tr>
        <tr>
          <th scope="row">2位</th>
          <td><%= school_rank.qt1_rank2 %></td>
        </tr>
        <tr>
          <th scope="row">3位</th>
          <td><%= school_rank.qt1_rank3 %></td>
        </tr>
      </table>
      <table class="table">
        <tr>
          <th scope="row">質問2</th>
          <td><%= school_rank.qestion2 %></td>
        </tr>
        <tr>
          <th scope="row">1位</th>
          <td><%= school_rank.qt2_rank1 %></td>
        </tr>
        <tr>
          <th scope="row">2位</th>
          <td><%= school_rank.qt2_rank2 %></td>
        </tr>
        <tr>
          <th scope="row">3位</th>
          <td><%= school_rank.qt2_rank3 %></td>
        </tr>
      </table>
      <table class="table">
        <tr>
          <th scope="row">一言</th>
          <td><%= school_rank.content %></td>
        </tr>
      </table>
    <% end %>
  
  </div>
