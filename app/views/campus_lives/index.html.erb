<p id="notice"><%= notice %></p>

<h1>Campus Lives</h1>

<table>
  <thead>
    <tr>
      <th>Image1</th>
      <th>Image2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campus_lives.each do |campus_life| %>
      <tr>
        <td><%= campus_life.image1 %></td>
        <td><%= campus_life.image2 %></td>
        <td><%= link_to 'Show', campus_life %></td>
        <td><%= link_to 'Edit', edit_campus_life_path(campus_life) %></td>
        <td><%= link_to 'Destroy', campus_life, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Campus Life', new_campus_life_path %>
