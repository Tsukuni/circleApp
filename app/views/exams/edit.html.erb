<div class="col-md-12">
  <h3>素材画像</h3>
</div>
<div>
  <%=form_for @exam do |f| %>
    <div class="col-md-6 adBox">
      <table class="table">
        <tr>
          <th scope="row">画像</th>
          <td><%= f.file_field :exam_image %></td>
        </tr>
      </table>
      <%= f.submit "送信" ,class: "btn" %>
    </div>
  <% end %>
</div>
