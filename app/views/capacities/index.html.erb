<h3>資格・就活</h3>
<div class="col-md-5 adBox">
  <h4>素材画像</h4>
  <% @capacities.each do |obj| %>
    <table class="table">
      <tr>
        <th scope="row">画像</th>
        <td><%= obj.image %><span class="button"><%= link_to '削除',capacity_path(obj.id),method: :delete%></span>
        <span class="button"><%= link_to '編集',edit_capacity_path(obj.id)%></td>
      </tr>
    </table>
  <%end%>
  <%= link_to '新規作成',new_capacity_path%>
</div>
